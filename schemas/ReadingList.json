{
  "readingList": {
    "type": "array",
    "items": {
      "type": "object",
      "properties": {
        "title": {
          "type": "string",
          "maxLength": 100
        },
        "author": {
          "type": "string",
          "maxLength": 100
        },
        "rating": {
          "type": "integer",
          "minimum": 1,
          "maximum": 5
        },
        "review": {
          "type": "string",
          "maxLength": 500
        }
      },
      "required": ["title", "author", "rating", "review"],
      "additionalProperties": false
    }
  }
}
